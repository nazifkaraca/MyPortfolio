@model List<MyPortfolio.DAL.Entities.Skill>

<div class="col-six tab-full right">
    <h3>Programlama Dilleri & Teknolojiler</h3>
    @foreach (var item in Model)
    {
        <ul class="skill-bars">
            <li>
                <div class="progress percent90"><span>@item.Value%</span></div>
                <strong>@item.Title</strong>
            </li>
        </ul>
    }
    
</div>  